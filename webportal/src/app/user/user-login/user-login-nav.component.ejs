<% if (cookies.get("token")) { %>
  <li class="dropdown">
    <a href="#" class="dropdown-toggle" data-toggle="dropdown">
      <img src="<%= cookies.get("headImage") %>" style="height: 32px;width: 30px"/>
    </a>
    <ul class="dropdown-menu">
      <li><a href="#" onclick="window.location.replace('/change-password.html')">修改密码</a></li>
      <li><a href="#" onclick="userLogout()">退出</a></li>
    </ul>
  </li>
<% } else { %>
  <li>
    <a href="#" data-toggle="control-sidebar" onclick="window.location.replace('/login.html')">
      <i class="fa fa-sign-in"></i>
      <span>Login</span>
    </a>
  </li>
<% } %>